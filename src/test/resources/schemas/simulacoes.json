{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "nome",
    "cpf",
    "email",
    "valor",
    "parcelas",
    "seguro"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        15
      ]
    },
    "nome": {
      "type": "string",
      "default": "",
      "title": "The nome Schema",
      "examples": [
        "Fulano de tal"
      ]
    },
    "cpf": {
      "type": "string",
      "default": "",
      "title": "The cpf Schema",
      "examples": [
        "17822386033"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "json@email.com"
      ]
    },
    "valor": {
      "type": "integer",
      "default": 0,
      "title": "The valor Schema",
      "examples": [
        1000
      ]
    },
    "parcelas": {
      "type": "integer",
      "default": 0,
      "title": "The parcelas Schema",
      "examples": [
        3
      ]
    },
    "seguro": {
      "type": "boolean",
      "default": false,
      "title": "The seguro Schema",
      "examples": [
        true
      ]
    }
  },
  "examples": [{
    "id": 15,
    "nome": "Fulano de tal",
    "cpf": "17822386033",
    "email": "json@email.com",
    "valor": 1000,
    "parcelas": 3,
    "seguro": true
  }]
}